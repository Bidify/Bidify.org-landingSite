# Environment variables for Bidify deployment
# Copy this file to .env and update with your values

# Traefik ACME email for Let's Encrypt certificates
# Required for SSL certificate generation
ACME_EMAIL=your-email@example.com

# Optional: Analytics tracking ID
# Add your Google Analytics or other tracking ID here
# ANALYTICS_ID=UA-XXXXXXXXX-X

# Optional: Custom headers
# Add any custom headers needed for your deployment
# CUSTOM_HEADER_NAME=value

# Optional: Logging level
# Available levels: debug, info, warn, error
LOG_LEVEL=info

# Optional: Enable/disable development features
# Set to false for production
DEBUG_MODE=false